<div class="review-container">
<a href="/places/<%= place.id %>/review">
  <button class="button--bordered-primary">Add review</button>
</a>
<% place.reviews.forEach((review) => { %>
  <article class="review-submission">
    <h1 class="review-submission__reviewer">
      <%= review.reviewerName %>
    </h1>
    <div class="place-card__star-rating" aria-label="<%= review.rating %>">
      <%- review.numberOfStars %>
    </div>
      <h2 class="review-submission__title">Comments</h2>
      <p class="review-submission__review">
        <%= review.reviewText %>
      </p>
      <!--loop review image file names-->
      <% if(review.reviewImageFileName !== '') { %>
        <div class="review-submission__image-container">
          <img class="review-submission__image" src="../../images/reviews/<%= review.reviewImageFileName %>" alt="<%= review.reviewImageAlt %>">
        </div>
      <% }; %>
  </article>
<% }); %>
</div>
